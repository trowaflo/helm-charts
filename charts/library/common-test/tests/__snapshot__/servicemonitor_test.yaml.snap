should create ServiceMonitor when enabled:
  1: |
    apiVersion: v1
    kind: Service
    metadata:
      labels:
        app.kubernetes.io/instance: common-test
        app.kubernetes.io/managed-by: Helm
        app.kubernetes.io/name: service-monitor-name-override
        helm.sh/chart: common-test-0.1.0
      name: common-test-service-monitor-name-override-main
      namespace: common-test-namespace-override-test
    spec:
      ports:
        - name: main
          port: 80
          protocol: TCP
          targetPort: main
      selector:
        app.kubernetes.io/instance: common-test
        app.kubernetes.io/name: service-monitor-name-override
      type: ClusterIP
  2: |
    apiVersion: networking.k8s.io/v1
    kind: Ingress
    metadata:
      labels:
        app.kubernetes.io/instance: common-test
        app.kubernetes.io/managed-by: Helm
        app.kubernetes.io/name: service-monitor-name-override
        helm.sh/chart: common-test-0.1.0
        test: true
      name: common-test-service-monitor-name-override
      namespace: common-test-namespace-override-test
    spec:
      ingressClassName: traefik
      rules:
        - host: chart-example.local
          http:
            paths:
              - backend:
                  service:
                    name: common-test-service-monitor-name-override-main
                    port:
                      name: main
                path: /
                pathType: Prefix
              - backend:
                  service:
                    name: common-test-service-monitor-name-override-main
                    port:
                      name: main
                path: /testpath
                pathType: Prefix
        - host: another-chart-example.local
          http:
            paths:
              - backend:
                  service:
                    name: common-test-service-monitor-name-override-main
                    port:
                      name: main
                path: /
                pathType: Prefix
              - backend:
                  service:
                    name: common-test-service-monitor-name-override-main
                    port:
                      name: main
                path: /testpath
                pathType: Prefix
  3: |
    apiVersion: apps/v1
    kind: Deployment
    metadata:
      labels:
        app.kubernetes.io/instance: common-test
        app.kubernetes.io/managed-by: Helm
        app.kubernetes.io/name: service-monitor-name-override
        helm.sh/chart: common-test-0.1.0
      name: common-test-service-monitor-name-override
      namespace: common-test-namespace-override-test
    spec:
      replicas: 1
      revisionHistoryLimit: 3
      selector:
        matchLabels:
          app.kubernetes.io/instance: common-test
          app.kubernetes.io/name: service-monitor-name-override
      strategy:
        type: Recreate
      template:
        metadata:
          annotations: null
          labels:
            app.kubernetes.io/instance: common-test
            app.kubernetes.io/name: service-monitor-name-override
        spec:
          automountServiceAccountToken: false
          containers:
            - args:
                - --test-arg=value
              env:
                - name: envtest
                  value: thevalue
                - name: valuefrom
                  valueFrom:
                    secretKeyRef:
                      key: password
                      name: my-secret
              image: traefik/whoami:v1.10@sha256:1699d99cb4b9acc17f74ca670b3d8d0b7ba27c948b3445f0593b58ebece92f04
              imagePullPolicy: Always
              livenessProbe:
                failureThreshold: 3
                httpGet:
                  path: /health
                  port: main
                  scheme: HTTP
                initialDelaySeconds: 30
                periodSeconds: 10
                successThreshold: 1
                timeoutSeconds: 5
              name: main
              ports:
                - containerPort: 80
                  name: main
                  protocol: TCP
              readinessProbe:
                failureThreshold: 3
                httpGet:
                  path: /health
                  port: main
                  scheme: HTTP
                initialDelaySeconds: 30
                periodSeconds: 10
                successThreshold: 1
                timeoutSeconds: 5
              resources:
                limits:
                  cpu: 2
                  memory: 2Gi
                requests:
                  cpu: 1m
                  memory: 32Mi
              securityContext:
                allowPrivilegeEscalation: false
                capabilities:
                  drop:
                    - ALL
                readOnlyRootFilesystem: true
                runAsNonRoot: true
                runAsUser: 65534
                seccompProfile:
                  type: RuntimeDefault
              startupProbe:
                failureThreshold: 3
                httpGet:
                  path: /health
                  port: main
                  scheme: HTTP
                initialDelaySeconds: 60
                periodSeconds: 10
                successThreshold: 1
                timeoutSeconds: 5
